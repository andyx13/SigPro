{
    "name": "sigpro.process_signals",
    "primitive": "sigpro.process_signals",
    "classifiers": {
        "type": "preprocessor",
        "subtype": "feature_extractor"
    },
    "produce": {
        "args": [
            {
                "name": "readings",
                "keyword": "data",
                "type": "pandas.DataFrame"
            },
            {
                "name": "feature_columns",
                "default": null,
                "type": "list"
            }
        ],
        "output": [
            {
                "name": "readings",
                "type": "pandas.DataFrame"
            },
            {
                "name": "feature_columns",
                "type": "list"
            }
        ]
    },
    "hyperparameters": {
        "fixed": {
            "keep_columns": {
                "type": "bool or list",
                "default": false
            },
            "values_column_name": {
                "type": "str",
                "default": "values"
            },
            "transformations": {
                "type": "list"
            },
            "aggregations": {
                "type": "list"
            }
        }
    }
}
